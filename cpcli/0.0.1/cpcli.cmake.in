# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(cpcli-0.0.1-install)

set(CPPM_VERSION 0.0.13)
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(cpcli 0.0.1  URL https://github.com/injae/cpcli/archive/refs/tags/v0.0.1.zip  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} )

