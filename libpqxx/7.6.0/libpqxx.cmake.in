# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(libpqxx-7.6.0-install)

set(CPPM_VERSION 0.0.13)
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(libpqxx 7.6.0  URL https://github.com/jtv/libpqxx/archive/refs/tags/7.6.0.zip  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} )

